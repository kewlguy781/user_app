<h1>Users#index</h1>

<h1> users </h1>
<% @users.each do |user| %>
  <h2><%=user.first_name %> <%= user.last_name %> </h2><br />
  Age: <%= user.age %>
  Hair Color: <%= user.hair_color %>
  Eye Color: <%= user.eye_color %>
  Gender: <%= user.gender %>
  Alive: <%= user.alive %>
  <%= link_to 'User Profile', user_path(user) %>
  <%= link_to 'New User', new_user_path %>

  <%= link_to 'Edit User', edit_user_path(user) %>
  <%= link_to 'Delete User', user, method: :delete %>

  <% end %>
